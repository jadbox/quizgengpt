{
  "extends": "astro/tsconfigs/strict",
  "include": ["src", "astro.config.mjs"],
  "compilerOptions": {
    "verbatimModuleSyntax": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
