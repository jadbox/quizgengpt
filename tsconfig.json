{
  "extends": "astro/tsconfigs/strict",
  "include": ["src", "astro.config.mjs"],
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "verbatimModuleSyntax": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true
  }
}
